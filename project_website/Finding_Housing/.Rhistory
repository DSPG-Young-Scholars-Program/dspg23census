geom_sf() +
labs(x='Latitude', y='Longitude', title='Population by Census tract in Fairfax county')
# plot population by census tract
ggplot(data = acs, aes(geometry = geometry, fill = B01001_001E)) +
geom_sf() +
labs(x='Latitude', y='Longitude', title='Population by Census tract in Fairfax county')
